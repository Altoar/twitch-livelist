{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "externally_connectable": {
    "matches": ["http://localhost:5173/*"]
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "service-worker.js"
  }
}
